  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.0",
    "cookie-session": "^2.0.0-beta.3",
    "express": "^4.16.2",
    "mongoose": "^4.13.2",
    "nodemon": "^1.12.1",
    "passport": "^0.4.0",
    "passport-google-oauth20": "^1.0.0",
    "sendgrid": "^5.2.3",
    "stripe": "^5.3.0"
